<?xml version="1.0" encoding="UTF-8"?>
<transformation>
  <info>
    <name/>
    <directory>/</directory>
    <log>
      <read/>
      <write/>
      <input/>
      <output/>
      <update/>
      <connection/>
      <table/>
      <use_batchid>Y</use_batchid>
      <use_logfield>N</use_logfield>
      </log>
    <maxdate>
      <connection/>
      <table/>
      <field/>
      <offset>0.0</offset>
      <maxdiff>0.0</maxdiff>
      </maxdate>
    <size_rowset>350</size_rowset>
    <sleep_time_empty>1</sleep_time_empty>
    <sleep_time_full>1</sleep_time_full>
    <dependencies>
      </dependencies>
    </info>
  <notepads>
    <notepad>
      <note>Determine the directory of this transformation
and add the filename.</note>
      <xloc>37</xloc>
      <yloc>53</yloc>
      <width>231</width>
      <heigth>36</heigth>
      </notepad>
    <notepad>
      <note>To see the result, do a preview on this Dummy step
... or add an output step.</note>
      <xloc>407</xloc>
      <yloc>192</yloc>
      <width>257</width>
      <heigth>36</heigth>
      </notepad>
    <notepad>
      <note>Load the example dBase file.</note>
      <xloc>299</xloc>
      <yloc>54</yloc>
      <width>148</width>
      <heigth>23</heigth>
      </notepad>
    <notepad>
      <note>Revision notes:
2006-08-07 Jens Bleuel: created</note>
      <xloc>14</xloc>
      <yloc>567</yloc>
      <width>166</width>
      <heigth>36</heigth>
      </notepad>
    </notepads>
  <order>
  <hop> <from>GetFilenameKTR</from><to>Get example filename</to><enabled>Y</enabled> </hop>  <hop> <from>Get example filename</from><to>XBase input</to><enabled>Y</enabled> </hop>  <hop> <from>XBase input</from><to>Dummy (do nothing)</to><enabled>Y</enabled> </hop>  </order>

  <step>
    <name>Dummy (do nothing)</name>
    <type>Dummy</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <GUI>
      <xloc>520</xloc>
      <yloc>104</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Get example filename</name>
    <type>ScriptValue</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <script>var fileSeparator = Packages.be.ibridge.kettle.core.Const.FILE_SEPARATOR;
var subDirEndsAt = FilenameKTR.getString().lastIndexOf(fileSeparator);
var subDir = FilenameKTR.getString().substr(0, subDirEndsAt+1);
var filenameDBF = subDir + &quot;XBase - dBase sample data.dbf&quot;

</script>
    <fields>      <field>        <name>filenameDBF</name>
        <rename>filenameDBF</rename>
        <type>String</type>
        <length>-1</length>
        <precision>-1</precision>
        </field>      </fields>    <GUI>
      <xloc>220</xloc>
      <yloc>105</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>GetFilenameKTR</name>
    <type>SystemInfo</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <fields>
      <field>
        <name>FilenameKTR</name>
        <type>transformation file name</type>
        </field>
      </fields>
    <GUI>
      <xloc>71</xloc>
      <yloc>105</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>XBase input</name>
    <type>XBaseInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <file_dbf/>
    <limit>0</limit>
    <add_rownr>N</add_rownr>
    <field_rownr/>
    <include>N</include>
    <include_field/>
    <accept_filenames>Y</accept_filenames>
    <accept_field>filenameDBF</accept_field>
    <accept_stepname>Get example filename</accept_stepname>
    <GUI>
      <xloc>351</xloc>
      <yloc>104</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

</transformation>
