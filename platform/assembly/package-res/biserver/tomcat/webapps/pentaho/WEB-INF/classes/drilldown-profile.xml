<?xml version="1.0" encoding="UTF-8"?>
<drilldown-profiles xmlns="http://reporting.pentaho.org/namespaces/engine/classic/drilldown-profile/1.0">
  <group name="self">
    <drilldown-profile name="self"
                       class="org.pentaho.reporting.engine.classic.extensions.drilldown.FormulaLinkCustomizer"
                       bundle-name="org.pentaho.reporting.engine.classic.extensions.drilldown.drilldown-profile"
                       expert="false" hidden="false" deprecated="false" preferred="false">
      <attribute name="formula">URLPARAMETERSEPARATOR(ENV(&quot;selfURL&quot;)) &amp; [&quot;::parameter&quot;]</attribute>
    </drilldown-profile>
  </group>
  <group name="url">
    <drilldown-profile name="generic-url" class="org.pentaho.reporting.engine.classic.extensions.drilldown.PatternLinkCustomizer"
                       bundle-name="org.pentaho.reporting.engine.classic.extensions.drilldown.drilldown-profile"
                       expert="false" hidden="false" deprecated="false" preferred="false">
      <attribute name="pattern">{0}?{1}</attribute>
    </drilldown-profile>
    <drilldown-profile name="local-url" class="org.pentaho.reporting.engine.classic.extensions.drilldown.FormulaLinkCustomizer"
                       bundle-name="org.pentaho.reporting.engine.classic.extensions.drilldown.drilldown-profile"
                       expert="false" hidden="false" deprecated="false" preferred="false">
      <attribute name="formula">IF(ISEMPTY([&quot;::parameter&quot;]); ENV(&quot;serverBaseURL&quot;) &amp; [&quot;::path&quot;]);
      URLPARAMETERSEPARATOR(ENV(&quot;serverBaseURL&quot;) &amp; [&quot;::path&quot;]) &amp; [&quot;::parameter&quot;])</attribute>
    </drilldown-profile>
  </group>
  <group name="pentaho">
    <drilldown-profile name="remote-xaction" class="org.pentaho.reporting.engine.classic.extensions.drilldown.FormulaLinkCustomizer"
                       bundle-name="org.pentaho.reporting.engine.classic.extensions.drilldown.drilldown-profile"
                       expert="false" hidden="false" deprecated="false" preferred="false">
      <attribute name="extension">xaction</attribute>
      <attribute name="formula">URLBUILDER([&quot;::path&quot;] ; &quot;/api/repos/&quot; &amp; [&quot;::resource&quot;] &amp; &quot;/generatedContent&quot; ; [&quot;::parameter&quot;])</attribute>
    </drilldown-profile>
    <drilldown-profile name="remote-prpt" class="org.pentaho.reporting.engine.classic.extensions.drilldown.FormulaLinkCustomizer"
                       bundle-name="org.pentaho.reporting.engine.classic.extensions.drilldown.drilldown-profile"
                       expert="false" hidden="false" deprecated="false" preferred="false">
      <attribute name="extension">prpt</attribute>
      <attribute name="formula">URLBUILDER([&quot;::path&quot;] ; &quot;/api/repos/&quot; &amp; [&quot;::resource&quot;] &amp; &quot;/viewer&quot; ; [&quot;::parameter&quot;])</attribute>
    </drilldown-profile>
    <drilldown-profile name="remote-prpt-no-parameter" class="org.pentaho.reporting.engine.classic.extensions.drilldown.FormulaLinkCustomizer"
                       bundle-name="org.pentaho.reporting.engine.classic.extensions.drilldown.drilldown-profile"
                       expert="false" hidden="false" deprecated="false" preferred="false">
      <attribute name="extension">prpt</attribute>
      <attribute name="formula">URLBUILDER([&quot;::path&quot;] ; &quot;/api/repos/&quot; &amp; [&quot;::resource&quot;] &amp; &quot;/viewer&quot; ; &quot;showParameters=false&amp;&quot; &amp; [&quot;::parameter&quot;])</attribute>
    </drilldown-profile>
    <drilldown-profile name="local-xaction" class="org.pentaho.reporting.engine.classic.extensions.drilldown.FormulaLinkCustomizer"
                       bundle-name="org.pentaho.reporting.engine.classic.extensions.drilldown.drilldown-profile"
                       expert="false" hidden="false" deprecated="false" preferred="false">
      <attribute name="extension">xaction</attribute>
      <attribute name="formula">URLBUILDER(ENV(&quot;pentahoBaseURL&quot;) ; &quot;/api/repos/&quot; &amp; [&quot;::resource&quot;] &amp; &quot;/generatedContent&quot; ; [&quot;::parameter&quot;])</attribute>
    </drilldown-profile>
    <drilldown-profile name="local-prpt"
                       class="org.pentaho.reporting.engine.classic.extensions.drilldown.FormulaLinkCustomizer"
                       bundle-name="org.pentaho.reporting.engine.classic.extensions.drilldown.drilldown-profile"
                       expert="false" hidden="false" deprecated="false" preferred="false">
      <attribute name="extension">prpt</attribute>
      <attribute name="formula">URLBUILDER(ENV(&quot;pentahoBaseURL&quot;) ; &quot;/api/repos/&quot; &amp; [&quot;::resource&quot;] &amp; &quot;/viewer&quot; ; [&quot;::parameter&quot;])</attribute>
    </drilldown-profile>
    <drilldown-profile name="local-prpt-no-parameter"
                       class="org.pentaho.reporting.engine.classic.extensions.drilldown.FormulaLinkCustomizer"
                       bundle-name="org.pentaho.reporting.engine.classic.extensions.drilldown.drilldown-profile"
                       expert="false" hidden="false" deprecated="false" preferred="false">
      <attribute name="extension">prpt</attribute>
      <attribute name="formula">URLBUILDER(ENV(&quot;pentahoBaseURL&quot;) ; &quot;/api/repos/&quot; &amp; [&quot;::resource&quot;] &amp; &quot;/viewer&quot; ; &quot;showParameters=false&amp;&quot; &amp; [&quot;::parameter&quot;])</attribute>
    </drilldown-profile>
  </group>
</drilldown-profiles>
