<module rename-to="reportviewer">

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User'/>
	<inherits name='com.google.gwt.xml.XML'/>

	<inherits name='org.pentaho.gwt.widgets.client.datepicker.DatePicker' />
	<inherits name='org.pentaho.gwt.widgets.client.utils.i18n.I18N'/>
	<inherits name='org.pentaho.gwt.widgets.client.utils.string.StringUtils' />
	
    <set-property name="gwt.suppressNonStaticFinalFieldWarnings" value="true" /> 

    <!-- This set of locales is here to support various number, date and currency formats -->
    <extend-property name="locale" values="ar"/>
    <extend-property name="locale" values="de"/>
    <extend-property name="locale" values="en"/>
    <extend-property name="locale" values="en_GB"/>
    <extend-property name="locale" values="en_US"/>
    <extend-property name="locale" values="es"/>
    <extend-property name="locale" values="fi"/>
    <extend-property name="locale" values="fr"/>
    <extend-property name="locale" values="it"/>
    <extend-property name="locale" values="ja"/>
    <extend-property name="locale" values="ko"/>
    <extend-property name="locale" values="nl"/>
    <extend-property name="locale" values="no"/>
    <extend-property name="locale" values="pt"/>
    <extend-property name="locale" values="ru"/>
    <extend-property name="locale" values="sv"/>
    <extend-property name="locale" values="zh_CN"/>
    <extend-property name="locale" values="zh_TW"/>
    
	<!-- Specify the app entry point class.                   -->
	<entry-point class='org.pentaho.reporting.platform.plugin.gwt.client.ReportViewer'/>
	
	<stylesheet src="report.css" />
	
</module>
