<?xml version="1.0" encoding="UTF-8"?>
<transformation>
  <info>
    <name/>
    <directory>/</directory>
    <log>
      <read/>
      <write/>
      <input/>
      <output/>
      <update/>
      <connection/>
      <table/>
      <use_batchid>Y</use_batchid>
      <use_logfield>N</use_logfield>
      </log>
    <maxdate>
      <connection/>
      <table/>
      <field/>
      <offset>0.0</offset>
      <maxdiff>0.0</maxdiff>
      </maxdate>
    <size_rowset>1000</size_rowset>
    <sleep_time_empty>1</sleep_time_empty>
    <sleep_time_full>1</sleep_time_full>
    <dependencies>
      </dependencies>
    </info>
  <notepads>
    </notepads>
  <order>
  <hop> <from>Generate Rows</from><to>Excel Output</to><enabled>Y</enabled> </hop>  </order>

  <step>
    <name>Excel Output</name>
    <type>ExcelOutput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <header>Y</header>
    <footer>N</footer>
    <encoding/>
    <file>
      <name>${Internal.Transformation.Filename.Directory}/output/datatypes</name>
      <extention>xls</extention>
      <split>N</split>
      <add_date>N</add_date>
      <add_time>N</add_time>
      <splitevery>75</splitevery>
      </file>
    <fields>
      <field>
        <name>StringField</name>
        <type>String</type>
        <format/>
        </field>
      <field>
        <name>NumberField</name>
        <type>Number</type>
        <format>#.000</format>
        </field>
      <field>
        <name>IntegerField</name>
        <type>Integer</type>
        <format/>
        </field>
      <field>
        <name>DateField</name>
        <type>Date</type>
        <format>M/d/yy H:mm:ss</format>
        </field>
      <field>
        <name>BooleanField</name>
        <type>Boolean</type>
        <format/>
        </field>
      <field>
        <name>BigNumberField</name>
        <type>BigNumber</type>
        <format/>
        </field>
      </fields>
    <GUI>
      <xloc>305</xloc>
      <yloc>73</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Generate Rows</name>
    <type>RowGenerator</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
    <fields>
      <field>
        <name>StringField</name>
        <type>String</type>
        <format/>
        <currency/>
        <decimal/>
        <group/>
        <nullif>This is a string field</nullif>
        <length>20</length>
        <precision>-1</precision>
        </field>
      <field>
        <name>NumberField</name>
        <type>Number</type>
        <format/>
        <currency/>
        <decimal>,</decimal>
        <group>.</group>
        <nullif>19348,392</nullif>
        <length>8</length>
        <precision>3</precision>
        </field>
      <field>
        <name>IntegerField</name>
        <type>Integer</type>
        <format/>
        <currency/>
        <decimal/>
        <group/>
        <nullif>38472</nullif>
        <length>7</length>
        <precision>0</precision>
        </field>
      <field>
        <name>DateField</name>
        <type>Date</type>
        <format>yyyy/MM/dd HH:mm:ss</format>
        <currency/>
        <decimal/>
        <group/>
        <nullif>2006/12/31 23:59:59</nullif>
        <length>-1</length>
        <precision>-1</precision>
        </field>
      <field>
        <name>BooleanField</name>
        <type>Boolean</type>
        <format/>
        <currency/>
        <decimal/>
        <group/>
        <nullif>TRUE</nullif>
        <length>-1</length>
        <precision>-1</precision>
        </field>
      <field>
        <name>BigNumberField</name>
        <type>BigNumber</type>
        <format/>
        <currency/>
        <decimal/>
        <group/>
        <nullif>989873947.2984839</nullif>
        <length>23</length>
        <precision>8</precision>
        </field>
      </fields>
    <limit>100</limit>
    <GUI>
      <xloc>103</xloc>
      <yloc>73</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

</transformation>
